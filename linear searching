#include<stdio.h>
#include<math.h>
void main()
{
 int a[20],i,n,num;
 printf("enter limit");
 scanf("%d",&n);
 for(i=0;i<n;i++)
 a[i]=rand()%100;
 printf("\nrandom numbers are");
 for(i=0;i<n;i++)
 printf("%d\t",a[i]);
 printf("\nenter search element");
 scanf("%d",&num);
 for(i=0;i<n;i++)
 {
  if(a[i]==num)
  break;
 }
 if(i==n)
 printf("\nnumber not found");
 else
 printf("\nnumber is found=%d",i);
} 
